#The rate card window rows
#This logic is when base volume is set as 80 pricing and ARC are set according to
#"lowerVolume":100,"endDate":"03-31-2018","rate":5,"upperVolume":105,"startDate":"03-01-2018"},
#"lowerVolume":105,"endDate":"03-31-2018","rate":10,"upperVolume":130,"startDate":"03-01-2018"},
#"lowerVolume":130,"endDate":"03-31-2018","rate":15,"startDate":"03-01-2018"}],

exp num of rows = 3
rate card row 1 = amount->15,charge_type->ARC,line_item_amount->15,pricingentitytype->,effectivedate->,invoicing_type->ARC/RRC,volume_band->Above 130,pricingentityid->,volume->1,pricingentityname->,currency_conversion_rate->1,rate->15,service_data_currency->INR
rate card row 2 = amount->200,charge_type->ARC,line_item_amount->200,pricingentitytype->,effectivedate->,invoicing_type->ARC/RRC,volume_band->Below 130,pricingentityid->,volume->20,pricingentityname->,currency_conversion_rate->1,rate->10,service_data_currency->INR
rate card row 3 = amount->20,charge_type->ARC,line_item_amount->20,pricingentitytype->,effectivedate->,invoicing_type->ARC/RRC,volume_band->Below 105,pricingentityid->,volume->4,pricingentityname->,currency_conversion_rate->1,rate->5,service_data_currency->INR




