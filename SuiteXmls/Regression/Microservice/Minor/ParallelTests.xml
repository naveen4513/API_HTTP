<?xml version="1.0" encoding="UTF-8"?>
<suite name="Parallel Tests" parallel="tests" thread-count="6">

    <groups>
        <run>
            <include name="minor"/>
        </run>
    </groups>

    <test name="Calendar Data Test">
        <classes>
            <class name="com.sirionlabs.test.calendar.TestCalendarData"></class>
        </classes>
    </test>

    <test name="Calendar Export Data Test">
        <classes>
            <class name="com.sirionlabs.test.calendar.TestCalendarExportData"></class>
        </classes>
    </test>

    <test name="Insights Validation">
        <classes>
            <class name="com.sirionlabs.test.insights.TestInsights"></class>
        </classes>
    </test>

</suite>